<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cs.db.house.Model.Str">

    <!-- Mapper -->
    <resultMap id="Str" type="Str" />

    <insert id="insertStr" parameterType="Str">
        INSERT INTO str (text) VALUES (#{text})
    </insert>

    <delete id="deleteStr" parameterType="int">
        DELETE FROM str WHERE id = #{id}
    </delete>

</mapper>